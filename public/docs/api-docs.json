    "paths":{
      "/api/auth/register":{
        "post":{
          "requestBody":{
            "content":{
              "application/json":{
                "schema":{
                  "type":"object",
                  "properties":{
                    "email":{
                      "default":"",
                      "example":"abc@abc.com",
                      "type":"string"
                    },
                    "name":{
                      "default":"",
                      "example":"sjahn",
                      "type":"string"
                    },
                    "password":{
                      "default":"",
                      "example":"1234",
