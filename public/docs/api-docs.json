            "content":{
              "application/json":{
                "schema":{
                  "type":"object",
                  "properties":{
                    "email":{
                      "default":"",
                      "example":"abc@abc.com",
                      "type":"string"
                    },
                    "password":{
                      "default":"",
                      "example":"1234",
                      "type":"string"
                    }
                  }
                }
              }
            },
            "required":true
          },
          "responses":{
            "200":{
              "description":"OK"
            }
          },
